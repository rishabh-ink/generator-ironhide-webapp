{
  "name": "<%= _.kebabCase(props.appname) %>",
  "description": "<%= props.description %>",
  "url": "https://github.com/<%= _.kebabCase(props.githubUsername) %>/<%= _.kebabCase(props.appname) %>",
  "license": "./license.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:<%= _.kebabCase(props.githubUsername) %>/<%= _.kebabCase(props.appname) %>.git"
  },
  "private": true,
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "dependencies": {
    "jquery": "https://github.com/jquery/jquery.git#1.10.2",
    "requirejs": "https://github.com/jrburke/requirejs-bower.git#2.1.17"
  },
  "devDependencies": {
    "jasmine-ajax": "https://github.com/jasmine/jasmine-ajax.git#3.1.1",
    "jasmine-jquery": "https://github.com/velesin/jasmine-jquery.git#2.1.0",
    "normalize-css": "https://github.com/necolas/normalize.css.git#3.0.3"
  },
  "resolutions": {
    "jquery": "1.10.2"
  },
  "ignore": [
    "**/.*",
    "/Gemfile",
    "/Gemfile.lock",
    "/gulpfile.js",
    "/libraries",
    "/node_modules",
    "/package.json",
    "/reports",
    "/tasks",
    "/tests"
  ],
  "main": [
    "dist/**/*",
    "bower.json",
    "readme.md",
    "license.md"
  ],
  "keywords": []
}
