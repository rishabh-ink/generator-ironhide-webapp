{
  "name": "<%= _.kebabCase(props.appname) %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  "url": "https://github.com/<%= _.kebabCase(props.githubUsername) %>/<%= _.kebabCase(props.appname) %>",
  "license": "./license.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:<%= _.kebabCase(props.githubUsername) %>/<%= _.kebabCase(props.appname) %>.git"
  },
  "private": true,
  "author": {
    "name": "",
    "email": "",
    "url": "https://github.com/<%= _.kebabCase(props.githubUsername) %>"
  },
  "dependencies": {
    "jquery": "https://github.com/jquery/jquery.git#~1.10",
    "requirejs": "https://github.com/jrburke/requirejs-bower.git#~2.1"
  },
  "devDependencies": {
    "jasmine-ajax": "https://github.com/jasmine/jasmine-ajax.git#~3.2",
    "jasmine-jquery": "https://github.com/velesin/jasmine-jquery.git#~2.1",
    "normalize-css": "https://github.com/necolas/normalize.css.git#~3.0",
    "voxel-hologram": "https://github.com/rishabhsrao/voxel-hologram.git#~1.4"
  },
  "resolutions": {
    "jquery": "~1.10"
  },
  "ignore": [
    "**/.*",
    "/Gemfile",
    "/Gemfile.lock",
    "/gulpfile.js",
    "/bower_components",
    "/node_modules",
    "/package.json",
    "/reports",
    "/tasks",
    "/tests"
  ],
  "main": [
    "dist/**/*",
    "bower.json",
    "readme.md",
    "license.md"
  ],
  "keywords": []
}
