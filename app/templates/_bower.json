{
  "name": "<%= _.kebabCase(props.appname) %>",
  "description": "<%= props.description %>",
  "url": "https://github.com/<%= _.kebabCase(props.githubUsername) %>/<%= _.kebabCase(props.appname) %>",
  "license": "./license.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:<%= _.kebabCase(props.githubUsername) %>/<%= _.kebabCase(props.appname) %>.git"
  },
  "private": true,
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "dependencies": {
    "jquery": "https://github.com/jquery/jquery.git#1.10.2"
  },
  "devDependencies": {
    "jasmine-ajax": "https://github.com/jasmine/jasmine-ajax.git#2.99.0",
    "jasmine-jquery": "https://github.com/velesin/jasmine-jquery.git#2.0.4",
    "normalize-css": "https://github.com/necolas/normalize.css.git#3.0.1"
  },
  "resolutions": {
    "jquery": "1.10.2"
  },
  "ignore": [
    ".bundle",
    ".git",
    ".sass-cache",
    "libraries",
    "tasks",
    "node_modules",
    "reports",
    "tests",
    "**/.*",
    "config.rb",
    "Gemfile",
    "Gemfile.lock",
    "gulpfile.js",
    "karma.conf.js",
    "package.json",
    "*.sublime*"
  ],
  "main": [
    "dist/**/*",
    "bower.json",
    "readme.md",
    "license.md"
  ],
  "keywords": []
}
